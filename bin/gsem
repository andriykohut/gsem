#!/usr/bin/env python3
import os
import sys

if __name__ == "__main__":
    libdir = os.path.abspath(os.path.join(os.path.dirname(os.path.realpath(__file__)), os.pardir))
    if os.path.exists(os.path.join(libdir, "gsem")):
        sys.path.insert(0, libdir)
    from gsem.cli import main
    main()
